    "preview:nextjs": "nx start nextjs-site",
    "preview:vite": "nx preview vite-app",
    "preview:snout": "nx serve snout",
    "test:all": "nx run-many -t test --parallel=3",
    "test:watch": "nx run-many -t test --watch",
    "lint:all": "nx run-many -t lint --parallel=3",
    "lint:fix": "nx run-many -t lint --fix --parallel=3",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "type-check": "nx run-many -t type-check --parallel=3",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "graph": "nx graph",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules apps/*/.next apps/*/dist apps/*/.astro",
    "reset": "nx reset"
  },
