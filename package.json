  "scripts": {
    "dev:all": "nx run-many -t dev storybook --parallel=4",
    "dev:components": "nx storybook @org/components",
    "dev:astro": "cd apps/astro-site && npm run dev",
    "dev:nextjs": "nx dev nextjs-site",
    "dev:vite": "nx serve vite-app",
    "dev:snout": "nx serve snout",
    "build:all": "nx run-many -t build build-storybook --parallel=4",
    "build:components": "nx build @org/components",
    "build:astro": "cd apps/astro-site && npm run build",
    "build:nextjs": "nx build nextjs-site",
    "build:vite": "nx build vite-app",
    "build:storybook": "nx build-storybook @org/components",
    "build:snout": "nx build snout",
